{
  "name": "japanese-calendar",
  "version": "0.0.1",
  "description": "An app meant to help practice Japanese calendar vocabulary",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch",
    "start-with-test": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist lint build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js"
  },
  "dependencies": {
    "babel-polyfill": "6.20.0",
    "jquery": "2.2.3",
    "react": "^15.6.2",
    "react-dom": "15.4.1",
    "react-redux": "^5.1.2",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.7",
    "redux": "^3.7.2",
    "redux-thunk": "2.1.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "6.18.0",
    "babel-core": "6.21.0",
    "babel-loader": "6.2.10",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.18.0",
    "colors": "1.1.2",
    "compression": "^1.7.4",
    "cross-env": "1.0.7",
    "css-loader": "^0.26.1",
    "enzyme": "2.2.0",
    "eslint": "3.12.2",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "6.8.0",
    "eslint-watch": "2.1.14",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.19.0",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "jsdom": "8.5.0",
    "mocha": "^7.1.1",
    "nock": "9.0.2",
    "node-sass": "^4.5.0",
    "npm-run-all": "3.1.2",
    "open": "0.0.5",
    "react-addons-test-utils": "15.4.1",
    "redux-immutable-state-invariant": "^2.0.0",
    "rimraf": "2.5.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-combine-loaders": "^2.0.3",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.13.2"
  }
}
